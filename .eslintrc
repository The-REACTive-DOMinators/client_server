{
  "parser": "@typescript-eslint/parser",
  "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
      "ecmaVersion": 2016,
      "sourceType": "module"
  },
  "rules": {
      "prettier/prettier": "error",
      "semi": [2, "always"],
      "eol-last": 0,
      "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }]
  }
}
